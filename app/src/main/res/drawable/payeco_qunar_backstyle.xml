<?xml version="1.0" encoding="UTF-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android">

    <item android:state_pressed="false"><shape>
            <corners android:radius="1px" />

            <gradient android:angle="270" android:endColor="#EAEBED" android:startColor="#EAEBED" />

            <stroke android:width="1px" android:color="#C8C7CC" />
        </shape></item>
    <item android:state_pressed="true"><shape>
            <corners android:radius="1px" />

            <gradient android:angle="270" android:endColor="#C8C7CC" android:startColor="#C8C7CC" />

            <stroke android:width="1px" android:color="#C8C7CC" />
        </shape></item>

</selector>